#export DSCCODE_ROOT_DIR=/home/joaoantoniocardoso/salome/Salome-V8_2_0-univ/modules/DSCCODE_V8_2_0
#export EFICAS_ROOT_DIR=/home/joaoantoniocardoso/salome/Salome-V8_2_0-univ/modules/EFICAS_V8_2_0
#export GEOM_ROOT_DIR=/home/joaoantoniocardoso/salome/Salome-V8_2_0-univ/modules/GEOM_V8_2_0
#export GHS3DPLUGIN_ROOT_DIR=/home/joaoantoniocardoso/salome/Salome-V8_2_0-univ/modules/GHS3DPLUGIN_V8_2_0
